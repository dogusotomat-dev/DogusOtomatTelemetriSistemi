# Firebase Configuration
# Copy this file to .env and replace with your actual Firebase project settings
# Never commit your actual .env file to version control

# Required Firebase Configuration (PLACEHOLDERS - REPLACE WITH YOUR ACTUAL VALUES)
REACT_APP_FIREBASE_API_KEY=                         # Firebase web API key (public)
REACT_APP_FIREBASE_AUTH_DOMAIN=                     # Firebase auth domain
REACT_APP_FIREBASE_DATABASE_URL=                    # Firebase database URL
REACT_APP_FIREBASE_PROJECT_ID=                      # Firebase project ID
REACT_APP_FIREBASE_STORAGE_BUCKET=                  # Firebase storage bucket
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=             # Firebase messaging sender ID
REACT_APP_FIREBASE_APP_ID=                          # Firebase app ID

# Optional Firebase Configuration (PLACEHOLDERS - REPLACE WITH YOUR ACTUAL VALUES)
REACT_APP_FIREBASE_MEASUREMENT_ID=                  # Firebase measurement ID

# Security Note:
# These environment variables are embedded in the client-side bundle during build
# Only use Firebase keys that are meant to be public (web API keys)
# Do not use Firebase service account keys or other sensitive credentials here